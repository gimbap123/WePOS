<mapper namespace="com.wepos.user.dao.UserDao">

	<insert id="insertProductGrade" parameterType="productGradeDto">
		insert into product_grade 
		values(#{userId}, #{productCode}, #{grade}, #{gradeComment})
	</insert>
	
</mapper>